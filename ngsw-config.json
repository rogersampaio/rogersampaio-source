{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://scripts.sirv.com/sirv.nospin.js",
          "https://rogersampaio.sirv.com/me.png?scale.option=fill&scale.width=44&scale.height=44",
          "https://rogersampaio.sirv.com/me.png?scale.option=fill&scale.width=88&scale.height=88&sharpen=20&quality=100&png.optimize=true",
          "https://rogersampaio.sirv.com/me.png?w=50&sharpen=20&q=100&png.optimize=true",
          "https://rogersampaio.sirv.com/me.png?w=50&h=50"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
